  "vs-kubernetes": {
    "vs-kubernetes.helm-path.mac": "/Users/nirwinkler/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path.mac": "/Users/nirwinkler/.vs-kubernetes/tools/draft/darwin-amd64/draft",
